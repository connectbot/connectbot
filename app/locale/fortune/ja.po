msgstr "'%1$s' から切断しますか?"

#. Shown in the alert dialog when a user chooses the 'delete host' item to
#. delete a host.
#, c-format
msgctxt "delete_host_confirm"
msgid "Are you sure you want to delete host '%1$s'?"
msgstr "ホスト'%1$s'を削除してよいですか?"

#. Menu selection to edit a pubkey details (e.g., nickname)
msgctxt "list_pubkey_edit"
msgid "Edit pubkey"
msgstr "公開鍵を編集"

#. ProxyJump / Jump Host support
#. Title for jump host selector in host editor
msgctxt "hostpref_jumphost_title"
msgid "Jump host (ProxyJump)"
msgstr "ジャンプホスト（ProxyJump）"

#. Summary text explaining jump host functionality
msgctxt "hostpref_jumphost_summary"
msgid "Connect through another SSH host"
msgstr "別のSSHホスト経由で接続する"

#. Option for no jump host (direct connection)
msgctxt "list_jumphost_none"
msgid "None (direct connection)"
msgstr "ダイレクト接続"

#. Terminal message when connecting via jump host
#, c-format
msgctxt "terminal_connecting_via_jump"
msgid "Connecting via jump host: %s"
msgstr "ジャンプホスト経由で接続する：%s"

#. Terminal message when connected to jump host
#, c-format
msgctxt "terminal_jump_connected"
msgid "Connected to jump host: %s"
msgstr "ジャンプホストに接続しました: %s"

#. Terminal message when authenticated to jump host
#, c-format
msgctxt "terminal_jump_authenticated"
msgid "Authenticated to jump host: %s"
msgstr "認証されたジャンプホスト：%s"

#. Terminal message when jump host connection fails
#, c-format
msgctxt "terminal_jump_failed"
msgid "Failed to connect to jump host %1$s: %2$s"
msgstr "ジャンプホスト%1$sへの接続に失敗しました：%2$s"

#. Terminal message when jump host authentication fails
#, c-format
msgctxt "terminal_jump_auth_failed"
msgid "Authentication failed for jump host: %s"
msgstr "認証に失敗しました ジャンプホスト %s に対して"

#. Terminal message when jump host is not found in database
msgctxt "terminal_jump_not_found"
msgid "Jump host not found in saved hosts"
msgstr "セーブされたホストにジャンプホストが見つかりません"

#. Password prompt prefix for jump host
#, c-format
msgctxt "terminal_jump_password"
msgid "Password for jump host %s:"
msgstr "パスワードを入力してください ジャンプホスト %s:"

#. Button text to view application logs for debugging
msgctxt "view_logs"
msgid "View Logs"
msgstr "ログの表示"

#. Title for the log viewer dialog
msgctxt "logs_title"
msgid "Application Logs"
msgstr "アプリケーションログ"

#. Button text to copy logs to clipboard for bug reporting
msgctxt "copy_logs"
msgid "Copy Logs"
msgstr "ログのコピー"

#. Confirmation message shown when logs are copied to clipboard
msgctxt "logs_copied"
msgid "Logs copied to clipboard"
msgstr "クリップボードにログをコピーしました"

#. Instruction text for reporting bugs with logs
msgctxt "logs_bug_report_info"
msgid ""
"To report a bug, copy these logs and open a new issue at "
"https://connectbot.org/bug"
msgstr "バグを報告するにはこれらのログをコピーしてhttps://connectbot.org/bugで新しい課題を開いてください"

#. Message shown when no logs are available
msgctxt "no_logs_available"
msgid "No logs available"
msgstr "ログは利用できない"

#. Section title for the "Help" screen showing information about the program
#. itself.
